            <sentence>
                <offset>960</offset>
                <text>Tanner's stage: M2, P2.</text>
            </sentence>
            <sentence>
                <offset>984</offset>
                <text>Intelligence was normal.</text>
            </sentence>
            <sentence>
                <offset>1009</offset>
                <text>Audiological evaluation disclosed bilateral mixed hearing loss.</text>
                <annotation id="20">
                    <infon key="type">webanno.custom.Phenotypes</infon>
