                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="Comment">! Other expertise [ENT]</infon>
                    <location length="11" offset="204"/>
                    <text>nasopharynx</text>
                </annotation>
                <annotation id="35">
