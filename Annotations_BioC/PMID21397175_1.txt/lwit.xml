                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0000402</infon>
                    <location length="17" offset="79"/>
                    <text>narrow ear canals</text>
                </annotation>
                <annotation id="17">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0000960</infon>
