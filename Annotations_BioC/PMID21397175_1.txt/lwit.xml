                </annotation>
                <annotation id="19">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0000048</infon>
                    <location length="13" offset="133"/>
