                </annotation>
                <annotation id="16">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="11" offset="53"/>
                    <text>extra digit</text>
                </annotation>
                <annotation id="18">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="15" offset="78"/>
