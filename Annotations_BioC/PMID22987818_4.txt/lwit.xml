                </annotation>
                <relation id="32">
                    <infon key="type">webanno.custom.PhenotypeRelations</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0001162</infon>
                    <node refid="30" role="source"/>
                    <node refid="31" role="target"/>
                </relation>
            </sentence>
            <sentence>
                <offset>1772</offset>
                <text>There was generalized brachydactyly and syndactyly.</text>
                <annotation id="33">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0001156</infon>
                    <location length="13" offset="22"/>
