                </annotation>
                <annotation id="8">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0000269</infon>
                    <location length="17" offset="40"/>
