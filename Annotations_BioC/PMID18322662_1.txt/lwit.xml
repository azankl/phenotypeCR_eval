                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0002947</infon>
                    <location length="17" offset="93"/>
                    <text>cervical kyphosis</text>
                </annotation>
                <annotation id="26">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0002650</infon>
