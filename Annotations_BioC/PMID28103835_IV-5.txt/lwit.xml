                </annotation>
                <annotation id="20">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0000508</infon>
                    <location length="28" offset="86"/>
                    <text>poor levator muscle function</text>
                </annotation>
                <annotation id="21">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="Comment">! Add term requested</infon>
                    <location length="21" offset="120"/>
                    <text>poor Bells phenomenon</text>
                </annotation>
            </sentence>
            <sentence>
                <offset>717</offset>
                <text>He was investigated at the ENT clinic for snoring, and found to have mild right eardrum retraction and a long uvula.</text>
                <annotation id="22">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0025267</infon>
                    <location length="7" offset="42"/>
                    <text>snoring</text>
                </annotation>
                <annotation id="23">
                    <infon key="type">webanno.custom.Phenotypes</infon>
