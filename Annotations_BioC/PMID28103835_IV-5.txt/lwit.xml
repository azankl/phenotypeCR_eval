                </annotation>
                <annotation id="26">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0000752</infon>
                    <location length="11" offset="22"/>
                    <text>hyperactive</text>
                </annotation>
            </sentence>
            <sentence>
                <offset>1087</offset>
