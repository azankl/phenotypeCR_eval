                </annotation>
                <annotation id="19">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="5" offset="134"/>
                    <text>small</text>
                </annotation>
                <annotation id="21">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="6" offset="146"/>
                    <text>kidney</text>
                </annotation>
