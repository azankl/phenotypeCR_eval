                </annotation>
                <annotation id="6">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0001114</infon>
                    <location length="21" offset="100"/>
