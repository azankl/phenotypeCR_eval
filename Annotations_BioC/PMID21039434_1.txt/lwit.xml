                </annotation>
                <annotation id="87">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0000269</infon>
                    <location length="17" offset="26"/>
                    <text>occipital bossing</text>
                </annotation>
                <annotation id="88">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="7" offset="36"/>
                    <text>bossing</text>
                </annotation>
                <annotation id="90">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0000260</infon>
