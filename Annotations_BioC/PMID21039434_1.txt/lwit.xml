                    <node refid="26" role="source"/>
                    <node refid="29" role="target"/>
                </relation>
            </sentence>
            <sentence>
                <offset>1651</offset>
                <text>The skin over the forehead was with a small capillary hemangioma and prominent vasculature, and abnormal pigmentation as in the original patients (2).</text>
                <annotation id="32">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="4" offset="4"/>
                    <text>skin</text>
                </annotation>
                <annotation id="33">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="8" offset="18"/>
                    <text>forehead</text>
                </annotation>
                <annotation id="34">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="20" offset="44"/>
                    <text>capillary hemangioma</text>
                </annotation>
                <annotation id="36">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0007394</infon>
                    <location length="21" offset="69"/>
                    <text>prominent vasculature</text>
                </annotation>
                <annotation id="37">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="21" offset="96"/>
