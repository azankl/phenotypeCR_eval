            <sentence>
                <offset>2980</offset>
                <text>The nose was wide and with a prominent nasal bridge and slightly anteverted nares.</text>
                <annotation id="66">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="4" offset="4"/>
