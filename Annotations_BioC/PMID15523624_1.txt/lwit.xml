                    <text>elbow</text>
                </annotation>
                <annotation id="82">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="4" offset="53"/>
                    <text>knee</text>
                </annotation>
                <relation id="79">
                    <infon key="type">webanno.custom.PhenotypeRelations</infon>
