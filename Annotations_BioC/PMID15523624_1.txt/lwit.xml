            </sentence>
            <sentence>
                <offset>336</offset>
                <text>Contractures were present at the wrists, and there were bilateral clubfeet.</text>
                <annotation id="17">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="12" offset="0"/>
                    <text>Contractures</text>
                </annotation>
                <annotation id="19">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="6" offset="33"/>
                    <text>wrists</text>
                </annotation>
                <annotation id="20">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0001762</infon>
                    <location length="8" offset="66"/>
                    <text>clubfeet</text>
                </annotation>
                <relation id="18">
                    <infon key="type">webanno.custom.PhenotypeRelations</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0001239</infon>
                    <node refid="19" role="source"/>
                    <node refid="17" role="target"/>
                </relation>
            </sentence>
            <sentence>
                <offset>412</offset>
                <text>A fracture of the left arm diagnosed immediately after birth was attributed to trauma during delivery.</text>
                <annotation id="21">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="Comment">! Unclear phenotype</infon>
                    <location length="8" offset="2"/>
                    <text>fracture</text>
                </annotation>
                <annotation id="23">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="3" offset="23"/>
