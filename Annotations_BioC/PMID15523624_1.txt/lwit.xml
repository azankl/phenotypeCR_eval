                </annotation>
                <relation id="22">
                    <infon key="type">webanno.custom.PhenotypeRelations</infon>
                    <node refid="23" role="source"/>
                    <node refid="21" role="target"/>
                </relation>
            </sentence>
            <sentence>
                <offset>515</offset>
                <text>Bilateral inguinal hernias were also diagnosed.</text>
                <annotation id="24">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0000023</infon>
                    <location length="16" offset="10"/>
                    <text>inguinal hernias</text>
                </annotation>
            </sentence>
            <sentence>
                <offset>563</offset>
                <text>In the first 3 months the boy had two more fractures, one in the left arm, the other in the right arm.</text>
                <annotation id="25">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="Comment">! Unclear phenotype</infon>
                    <location length="9" offset="43"/>
                    <text>fractures</text>
                </annotation>
                <annotation id="26">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="Comment">! Unclear phenotype</infon>
                    <location length="9" offset="43"/>
                    <text>fractures</text>
                </annotation>
                <annotation id="28">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="3" offset="70"/>
                    <text>arm</text>
                </annotation>
                <annotation id="30">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="3" offset="98"/>
                    <text>arm</text>
                </annotation>
