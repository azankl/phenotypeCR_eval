                </annotation>
            </sentence>
            <sentence>
                <offset>1260</offset>
                <text>Lungs were hypoplastic.</text>
                <annotation id="34">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="5" offset="0"/>
                    <text>Lungs</text>
                </annotation>
                <annotation id="35">
