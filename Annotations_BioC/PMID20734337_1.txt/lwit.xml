                <relation id="24">
                    <infon key="type">webanno.custom.PhenotypeRelations</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0002714</infon>
                    <node refid="22" role="source"/>
                    <node refid="23" role="target"/>
                </relation>
            </sentence>
            <sentence>
                <offset>933</offset>
                <text>Her neck was normal and there was no chest deformity.</text>
            </sentence>
            <sentence>
                <offset>987</offset>
                <text>She had a small, dystrophic left fifth toenail, but her digits and nails were otherwise unremarkable.</text>
                <annotation id="25">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="5" offset="10"/>
                    <text>small</text>
                </annotation>
                <annotation id="28">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="10" offset="17"/>
                    <text>dystrophic</text>
                </annotation>
                <annotation id="30">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="7" offset="39"/>
                    <text>toenail</text>
                </annotation>
                <annotation id="27">
