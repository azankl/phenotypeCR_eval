                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0011803</infon>
                    <node refid="13" role="source"/>
                    <node refid="14" role="target"/>
                </relation>
            </sentence>
            <sentence>
                <offset>821</offset>
                <text>Her ears were slightly low-set.</text>
                <annotation id="16">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="4" offset="4"/>
                    <text>ears</text>
                </annotation>
                <annotation id="17">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="7" offset="23"/>
                    <text>low-set</text>
                </annotation>
                <relation id="18">
                    <infon key="type">webanno.custom.PhenotypeRelations</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0000369</infon>
                    <node refid="16" role="source"/>
                    <node refid="17" role="target"/>
                </relation>
            </sentence>
            <sentence>
                <offset>853</offset>
                <text>The philtrum was relatively long and the mouth had slightly downturned corners.</text>
                <annotation id="19">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="8" offset="4"/>
                    <text>philtrum</text>
                </annotation>
