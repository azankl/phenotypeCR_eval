                    <location length="13" offset="37"/>
                    <text>desmoid tumor</text>
                </annotation>
            </sentence>
            <sentence>
                <offset>784</offset>
                <text>He had a glandular hypospaidias and bilateral orchidopexy at 8 years old.</text>
                <annotation id="29">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0000047</infon>
