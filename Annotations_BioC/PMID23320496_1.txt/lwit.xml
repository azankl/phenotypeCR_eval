                </annotation>
                <annotation id="19">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0000768</infon>
                    <location length="16" offset="229"/>
