                    <text>ulna</text>
                </annotation>
                <annotation id="16">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="5" offset="81"/>
                    <text>skull</text>
                </annotation>
                <relation id="15">
                    <infon key="type">webanno.custom.PhenotypeRelations</infon>
