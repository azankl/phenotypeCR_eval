                    <node refid="8" role="source"/>
                    <node refid="6" role="target"/>
                </relation>
            </sentence>
            <sentence>
                <offset>474</offset>
                <text>His length at birth was on the 97th centile.</text>
            </sentence>
            <sentence>
                <offset>522</offset>
                <text>He also had a left-sided inguinal hernia, undescended left testis, Fallot's tetralogy and scoliosis, all of which required surgical treatment.</text>
                <annotation id="12">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0000023</infon>
                    <location length="15" offset="25"/>
                    <text>inguinal hernia</text>
                </annotation>
                <annotation id="13">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="11" offset="42"/>
                    <text>undescended</text>
                </annotation>
                <annotation id="15">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="6" offset="59"/>
                    <text>testis</text>
                </annotation>
                <annotation id="16">
