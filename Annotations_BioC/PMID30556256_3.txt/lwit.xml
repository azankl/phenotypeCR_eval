            </sentence>
            <sentence>
                <offset>1130</offset>
                <text>He still has his primary dentition.</text>
                <annotation id="33">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="5" offset="3"/>
                    <text>still</text>
                </annotation>
