            <sentence>
                <offset>0</offset>
                <text>Patient 3 is the brother of Patients 1 and 2.</text>
            </sentence>
            <sentence>
                <offset>46</offset>
                <text>Antenatal ultrasound had demonstrated an abnormal bladder, polyhydramnios, kinking of the thoracic spine and an enlarged 3rd cerebral ventricle.</text>
                <annotation id="1">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0025487</infon>
                    <location length="16" offset="41"/>
                    <text>abnormal bladder</text>
                </annotation>
                <annotation id="2">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0001561</infon>
                    <location length="14" offset="59"/>
                    <text>polyhydramnios</text>
                </annotation>
                <annotation id="3">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0002943</infon>
                    <location length="29" offset="75"/>
