                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0002275</infon>
                    <location length="16" offset="56"/>
                    <text>motor impairment</text>
                </annotation>
                <annotation id="25">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="10" offset="62"/>
                    <text>impairment</text>
                </annotation>
                <relation id="24">
