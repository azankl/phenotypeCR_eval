                    <infon key="type">webanno.custom.PhenotypeRelations</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0004411</infon>
                    <node refid="34" role="source"/>
                    <node refid="35" role="target"/>
                </relation>
            </sentence>
            <sentence>
                <offset>940</offset>
                <text>Laboratory investigations revealed the hereditary coagulation abnormality von Willebrand disease.</text>
                <annotation id="37">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0008330</infon>
                    <location length="22" offset="74"/>
                    <text>von Willebrand disease</text>
                </annotation>
            </sentence>
            <sentence>
                <offset>1038</offset>
                <text>Further blood parameters showed no further anomalies.</text>
            </sentence>
            <sentence>
                <offset>1093</offset>
                <text>Teachers described the 9-year-old index patient as a distractible child who found it difficult to perform tasks requiring concentration.</text>
                <annotation id="38">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0000736</infon>
                    <location length="12" offset="53"/>
                    <text>distractible</text>
                </annotation>
                <annotation id="39">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="9" offset="85"/>
                    <text>difficult</text>
                </annotation>
                <annotation id="41">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="13" offset="122"/>
                    <text>concentration</text>
                </annotation>
                <relation id="40">
                    <infon key="type">webanno.custom.PhenotypeRelations</infon>
