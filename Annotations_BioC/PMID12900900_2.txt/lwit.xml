                </annotation>
                <annotation id="16">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="4" offset="135"/>
                    <text>high</text>
                </annotation>
                <annotation id="19">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0000189</infon>
                    <location length="13" offset="144"/>
