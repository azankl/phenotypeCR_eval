                <relation id="17">
                    <infon key="type">webanno.custom.PhenotypeRelations</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0000218</infon>
                    <node refid="18" role="source"/>
                    <node refid="16" role="target"/>
                </relation>
            </sentence>
            <sentence>
                <offset>1227</offset>
                <text>The umbilical stump was prominent.</text>
                <annotation id="22">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="15" offset="4"/>
                    <text>umbilical stump</text>
                </annotation>
                <annotation id="23">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="9" offset="24"/>
                    <text>prominent</text>
                </annotation>
                <relation id="24">
                    <infon key="type">webanno.custom.PhenotypeRelations</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0001544</infon>
                    <node refid="22" role="source"/>
                    <node refid="23" role="target"/>
                </relation>
            </sentence>
            <sentence>
                <offset>1262</offset>
                <text>The anus was anteriorly placed and with excessive tissue mounding.</text>
                <annotation id="25">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="4" offset="4"/>
                    <text>anus</text>
                </annotation>
                <annotation id="26">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="17" offset="13"/>
                    <text>anteriorly placed</text>
                </annotation>
                <relation id="27">
                    <infon key="type">webanno.custom.PhenotypeRelations</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0001545</infon>
                    <node refid="25" role="source"/>
                    <node refid="26" role="target"/>
                </relation>
            </sentence>
            <sentence>
                <offset>1329</offset>
                <text>A sacral dimple was present.</text>
                <annotation id="28">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="13" offset="2"/>
                    <text>sacral dimple</text>
                </annotation>
            </sentence>
            <sentence>
                <offset>1358</offset>
                <text>The palms and soles were furrowed and hyperkeratotic.</text>
                <annotation id="29">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="5" offset="4"/>
                    <text>palms</text>
                </annotation>
                <annotation id="30">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="5" offset="4"/>
                    <text>palms</text>
                </annotation>
