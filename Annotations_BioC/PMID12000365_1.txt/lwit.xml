                    <location length="4" offset="14"/>
                    <text>ears</text>
                </annotation>
                <annotation id="23">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="7" offset="24"/>
                    <text>low set</text>
                </annotation>
                <annotation id="25">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="9" offset="36"/>
                    <text>malformed</text>
                </annotation>
                <annotation id="27">
                    <infon key="type">webanno.custom.Phenotypes</infon>
