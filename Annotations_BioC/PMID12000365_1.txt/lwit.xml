                    <text>furrows</text>
                </annotation>
                <annotation id="29">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="13" offset="80"/>
                    <text>pre-auricular</text>
                </annotation>
                <relation id="24">
                    <infon key="type">webanno.custom.PhenotypeRelations</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0000369</infon>
                    <node refid="21" role="source"/>
