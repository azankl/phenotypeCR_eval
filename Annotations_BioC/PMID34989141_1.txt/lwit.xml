                    <node refid="24" role="target"/>
                </relation>
            </sentence>
            <sentence>
                <offset>1320</offset>
                <text>Fetal MRI examination confirmed short limbed fetus.</text>
                <annotation id="26">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0009826</infon>
                    <location length="12" offset="32"/>
                    <text>short limbed</text>
                </annotation>
            </sentence>
            <sentence>
                <offset>1372</offset>
                <text>Short and slightly bowed long bones of lower limbs were observed along with short long bones of upper limbs.</text>
                <annotation id="27">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="5" offset="0"/>
