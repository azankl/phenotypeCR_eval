                </annotation>
                <annotation id="20">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="31" offset="52"/>
                    <text>subluxation of the radial heads</text>
                </annotation>
            </sentence>
            <sentence>
                <offset>916</offset>
                <text>The vertebral contour was irregular, but clefting was not observed.</text>
                <annotation id="21">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="17" offset="4"/>
                    <text>vertebral contour</text>
                </annotation>
