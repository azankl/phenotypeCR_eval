                    <location length="23" offset="112"/>
                    <text>pulmonary vein stenosis</text>
                </annotation>
            </sentence>
            <sentence>
                <offset>247</offset>
                <text>She has global developmental delay and autism.</text>
                <annotation id="4">
                    <infon key="type">webanno.custom.Phenotypes</infon>
