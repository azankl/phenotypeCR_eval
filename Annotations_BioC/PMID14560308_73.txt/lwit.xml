            <sentence>
                <offset>987</offset>
                <text>At the time of our investigation, there was mild short stature (105 cm; 10th percentile), mild hyperextensible joints and posteriorly rotated low set ears.</text>
                <annotation id="23">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0004322</infon>
                    <location length="13" offset="49"/>
                    <text>short stature</text>
                </annotation>
                <annotation id="24">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0001382</infon>
                    <location length="22" offset="95"/>
                    <text>hyperextensible joints</text>
                </annotation>
                <annotation id="25">
                    <infon key="type">webanno.custom.Phenotypes</infon>
