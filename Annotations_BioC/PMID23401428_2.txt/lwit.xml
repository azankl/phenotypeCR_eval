                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0003049</infon>
                    <node refid="38" role="source"/>
                    <node refid="39" role="target"/>
                </relation>
            </sentence>
            <sentence>
                <offset>2321</offset>
                <text>There was very little ossification of the carpals and metacarpals of both hands (Fig. 5).</text>
                <annotation id="41">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0001216</infon>
                    <location length="34" offset="15"/>
                    <text>little ossification of the carpals</text>
                </annotation>
                <annotation id="42">
                    <infon key="type">webanno.custom.Phenotypes</infon>
