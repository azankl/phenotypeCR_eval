                </annotation>
                <annotation id="60">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="19" offset="48"/>
                    <text>absent ossification</text>
                </annotation>
                <annotation id="62">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="14" offset="88"/>
