                <annotation id="44">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="Comment">! Add term accepted, HP:6000651, https://github.com/obophenotype/human-phenotype-ontology/issues/10413</infon>
                    <location length="11" offset="54"/>
                    <text>metacarpals</text>
                </annotation>
                <relation id="43">
                    <infon key="type">webanno.custom.PhenotypeRelations</infon>
                    <node refid="44" role="source"/>
                    <node refid="42" role="target"/>
                </relation>
            </sentence>
            <sentence>
                <offset>2411</offset>
                <text>The sacrum was not well ossified.</text>
                <annotation id="45">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="6" offset="4"/>
                    <text>sacrum</text>
                </annotation>
