                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0005922</infon>
                    <location length="22" offset="46"/>
                    <text>deformity of the hands</text>
                </annotation>
                <annotation id="2">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="9" offset="46"/>
                    <text>deformity</text>
                </annotation>
                <annotation id="4">
