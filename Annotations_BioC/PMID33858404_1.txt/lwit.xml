            <offset>0</offset>
            <sentence>
                <offset>0</offset>
                <text>The 29-year-old male patient is the first child of healthy unrelated Lithuanian parents.</text>
            </sentence>
            <sentence>
                <offset>89</offset>
                <text>The proband was born at full term.</text>
            </sentence>
            <sentence>
                <offset>124</offset>
                <text>Psychomotor development of the proband was delayed.</text>
                <annotation id="1">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="23" offset="0"/>
                    <text>Psychomotor development</text>
                </annotation>
                <annotation id="2">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="7" offset="43"/>
