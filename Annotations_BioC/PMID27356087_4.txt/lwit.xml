            <sentence>
                <offset>579</offset>
                <text>He has an alveolar cleft with an anterior palatal fistula.</text>
                <annotation id="34">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0010289</infon>
