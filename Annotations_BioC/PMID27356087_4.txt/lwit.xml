                    <location length="15" offset="42"/>
                    <text>palatal fistula</text>
                </annotation>
            </sentence>
            <sentence>
                <offset>638</offset>
                <text>Speech evaluation reveals hypernasality.</text>
                <annotation id="36">
                    <infon key="type">webanno.custom.Phenotypes</infon>
