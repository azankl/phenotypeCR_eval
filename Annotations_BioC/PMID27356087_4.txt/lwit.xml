                    <node refid="43" role="target"/>
                </relation>
            </sentence>
            <sentence>
                <offset>893</offset>
                <text>There is asymmetry of the left naris.</text>
                <annotation id="45">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="9" offset="9"/>
                    <text>asymmetry</text>
