                    <location length="18" offset="35"/>
                    <text>facial dysmorphism</text>
                </annotation>
                <annotation id="4">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0000256</infon>
                    <location length="11" offset="71"/>
                    <text>macrocrania</text>
                </annotation>
                <annotation id="5">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0000316</infon>
                    <location length="13" offset="84"/>
                    <text>hypertelorism</text>
                </annotation>
                <annotation id="6">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="7" offset="99"/>
