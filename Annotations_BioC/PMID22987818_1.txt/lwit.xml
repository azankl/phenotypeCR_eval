                </annotation>
                <annotation id="14">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0009087</infon>
                    <location length="25" offset="49"/>
                    <text>posteriorly placed tongue</text>
                </annotation>
                <annotation id="15">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0000308</infon>
                    <location length="17" offset="79"/>
                    <text>microretrognathia</text>
                </annotation>
            </sentence>
            <sentence>
                <offset>650</offset>
                <text>He was also noted to have a small chest.</text>
                <annotation id="16">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0005257</infon>
                    <location length="11" offset="28"/>
