                <annotation id="28">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="8" offset="70"/>
                    <text>thoracic</text>
                </annotation>
                <relation id="24">
                    <infon key="type">webanno.custom.PhenotypeRelations</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0002943</infon>
                    <node refid="21" role="source"/>
                    <node refid="23" role="target"/>
                </relation>
                <relation id="26">
                    <infon key="type">webanno.custom.PhenotypeRelations</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0004626</infon>
                    <node refid="22" role="source"/>
                    <node refid="25" role="target"/>
