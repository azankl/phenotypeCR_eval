                    <node refid="58" role="source"/>
                    <node refid="59" role="target"/>
                </relation>
            </sentence>
            <sentence>
                <offset>2057</offset>
                <text>The ulnae were bowed bilaterally.</text>
                <annotation id="61">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="5" offset="4"/>
                    <text>ulnae</text>
                </annotation>
