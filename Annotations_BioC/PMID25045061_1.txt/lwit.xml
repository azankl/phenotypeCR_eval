                    <node refid="61" role="source"/>
                    <node refid="62" role="target"/>
                </relation>
            </sentence>
            <sentence>
                <offset>2091</offset>
                <text>Vertebral morphology was unremarkable, but the bone substance was again sclerotic.</text>
                <annotation id="64">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="9" offset="0"/>
                    <text>Vertebral</text>
                </annotation>
                <annotation id="65">
