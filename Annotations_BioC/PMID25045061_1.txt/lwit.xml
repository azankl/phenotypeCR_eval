                </relation>
            </sentence>
            <sentence>
                <offset>1938</offset>
                <text>The frontal sinuses were hypoplastic.</text>
                <annotation id="54">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="15" offset="4"/>
                    <text>frontal sinuses</text>
                </annotation>
