            </sentence>
            <sentence>
                <offset>1670</offset>
                <text>The diaphysis of the radius was bowed.</text>
                <annotation id="52">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="6" offset="21"/>
                    <text>radius</text>
                </annotation>
                <annotation id="53">
