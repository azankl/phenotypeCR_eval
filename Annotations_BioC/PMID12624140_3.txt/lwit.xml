                    <location length="7" offset="4"/>
                    <text>humerus</text>
                </annotation>
                <annotation id="48">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="5" offset="16"/>
                    <text>broad</text>
                </annotation>
                <annotation id="50">
                    <infon key="type">webanno.custom.Phenotypes</infon>
