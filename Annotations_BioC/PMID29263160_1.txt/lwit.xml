                </annotation>
                <annotation id="10">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0005280</infon>
                    <location length="17" offset="173"/>
                    <text>flat nasal bridge</text>
                </annotation>
                <annotation id="11">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0011263</infon>
                    <location length="17" offset="198"/>
