                </annotation>
                <annotation id="13">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0100807</infon>
                    <location length="12" offset="231"/>
                    <text>long fingers</text>
                </annotation>
                <annotation id="14">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0011304</infon>
                    <location length="12" offset="256"/>
                    <text>broad thumbs</text>
                </annotation>
