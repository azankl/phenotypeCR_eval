                    <location length="18" offset="34"/>
                    <text>motor developments</text>
                </annotation>
                <annotation id="17">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="7" offset="58"/>
                    <text>delayed</text>
                </annotation>
                <annotation id="19">
                    <infon key="type">webanno.custom.Phenotypes</infon>
