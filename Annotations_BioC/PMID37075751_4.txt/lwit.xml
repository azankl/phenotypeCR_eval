            <sentence>
                <offset>746</offset>
                <text>Echocardiogram demonstrated anomalous origin of the right coronary artery from the left cusp.</text>
                <annotation id="30">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="16" offset="28"/>
