                    <text>Vocal cord paralysis</text>
                </annotation>
                <annotation id="5">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0005950</infon>
