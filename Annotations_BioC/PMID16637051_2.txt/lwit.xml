                    <node refid="46" role="source"/>
                    <node refid="47" role="target"/>
                </relation>
            </sentence>
            <sentence>
                <offset>1422</offset>
                <text>Metacarpal bones were short.</text>
                <annotation id="49">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="10" offset="0"/>
                    <text>Metacarpal</text>
