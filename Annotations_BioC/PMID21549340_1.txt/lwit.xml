                    <node refid="20" role="source"/>
                    <node refid="21" role="target"/>
                </relation>
            </sentence>
            <sentence>
                <offset>848</offset>
                <text>There was limited supination at the elbow.</text>
                <annotation id="23">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0006394</infon>
                    <location length="31" offset="10"/>
                    <text>limited supination at the elbow</text>
                </annotation>
            </sentence>
            <sentence>
                <offset>891</offset>
                <text>Both knees showed valgus deformity.</text>
                <annotation id="24">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="5" offset="5"/>
                    <text>knees</text>
                </annotation>
                <annotation id="25">
