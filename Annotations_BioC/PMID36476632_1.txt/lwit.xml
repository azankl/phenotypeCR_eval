                    <text>knee</text>
                </annotation>
                <annotation id="14">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0001883</infon>
                    <location length="7" offset="136"/>
                    <text>talipes</text>
                </annotation>
                <relation id="4">
                    <infon key="type">webanno.custom.PhenotypeRelations</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0003026</infon>
