                    <node refid="28" role="source"/>
                    <node refid="29" role="target"/>
                </relation>
            </sentence>
            <sentence>
                <offset>1176</offset>
                <text>Endocrinological evaluation diagnosed pseudohypoparathyroidism and hypothyroidism.</text>
                <annotation id="31">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0000852</infon>
                    <location length="24" offset="38"/>
                    <text>pseudohypoparathyroidism</text>
                </annotation>
                <annotation id="32">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0000821</infon>
                    <location length="14" offset="67"/>
                    <text>hypothyroidism</text>
                </annotation>
            </sentence>
            <sentence>
                <offset>1259</offset>
                <text>At 11 years old, she had avascular necrosis of the right femur head.</text>
                <annotation id="33">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="18" offset="25"/>
                    <text>avascular necrosis</text>
                </annotation>
                <annotation id="35">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="10" offset="57"/>
