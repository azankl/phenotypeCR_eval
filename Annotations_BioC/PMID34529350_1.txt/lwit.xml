                </annotation>
                <annotation id="48">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0006387</infon>
                    <location length="49" offset="42"/>
