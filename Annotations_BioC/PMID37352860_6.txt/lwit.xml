                </annotation>
                <annotation id="7">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0001156</infon>
                    <location length="13" offset="56"/>
                    <text>brachydactyly</text>
                </annotation>
                <annotation id="8">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0002164</infon>
                    <location length="16" offset="80"/>
                    <text>dysplastic nails</text>
                </annotation>
            </sentence>
            <sentence>
                <offset>608</offset>
                <text>Feet examination showed short wide feet, short, low set 4th left toe, very slight partial syndactyly 2-3.</text>
