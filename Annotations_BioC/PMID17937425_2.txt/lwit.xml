            <sentence>
                <offset>770</offset>
                <text>There was no intellectual deficit.</text>
            </sentence>
            <sentence>
                <offset>805</offset>
                <text>Roentgenograms demonstrated bilateral radioulnar synostosis.</text>
                <annotation id="18">
                    <infon key="type">webanno.custom.Phenotypes</infon>
