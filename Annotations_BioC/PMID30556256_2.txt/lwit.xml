                    <node refid="21" role="source"/>
                    <node refid="19" role="target"/>
                </relation>
            </sentence>
            <sentence>
                <offset>955</offset>
                <text>There was also a bony bridge leading to posterior fusion of the left 7th and 8th ribs.</text>
                <annotation id="22">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="6" offset="50"/>
                    <text>fusion</text>
                </annotation>
                <annotation id="24">
