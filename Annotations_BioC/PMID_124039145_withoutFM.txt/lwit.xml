                    <node refid="39" role="source"/>
                    <node refid="37" role="target"/>
                </relation>
            </sentence>
            <sentence>
                <offset>1645</offset>
                <text>The neurocranium appeared small, there was thickening of the cranial vault, and the facial bones were more prominent compared to the neurocranium (Fig. 2A).</text>
                <annotation id="42">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="12" offset="4"/>
                    <text>neurocranium</text>
                </annotation>
                <annotation id="43">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="5" offset="26"/>
                    <text>small</text>
                </annotation>
                <annotation id="45">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0002684</infon>
                    <location length="31" offset="43"/>
                    <text>thickening of the cranial vault</text>
                </annotation>
                <annotation id="46">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="12" offset="84"/>
                    <text>facial bones</text>
                </annotation>
                <annotation id="47">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="9" offset="107"/>
                    <text>prominent</text>
                </annotation>
                <relation id="44">
                    <infon key="type">webanno.custom.PhenotypeRelations</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0000252</infon>
                    <node refid="42" role="source"/>
                    <node refid="43" role="target"/>
                </relation>
                <relation id="48">
