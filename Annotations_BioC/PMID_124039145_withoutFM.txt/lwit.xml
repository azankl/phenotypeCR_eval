                </annotation>
                <annotation id="40">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0003041</infon>
                    <location length="23" offset="100"/>
                    <text>humeroradial synostosis</text>
                </annotation>
                <annotation id="41">
                    <infon key="type">webanno.custom.Phenotypes</infon>
