                </annotation>
                <relation id="27">
                    <infon key="type">webanno.custom.PhenotypeRelations</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0005625</infon>
                    <node refid="25" role="source"/>
                    <node refid="26" role="target"/>
                </relation>
                <relation id="31">
