                </annotation>
                <annotation id="19">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="9" offset="66"/>
                    <text>prominent</text>
                </annotation>
                <annotation id="22">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0000431</infon>
                    <location length="18" offset="80"/>
