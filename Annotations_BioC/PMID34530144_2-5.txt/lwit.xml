                </annotation>
                <annotation id="28">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0003301</infon>
                    <location length="39" offset="81"/>
                    <text>endplate irregularities of the vertebra</text>
                </annotation>
                <annotation id="29">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0002938</infon>
                    <location length="20" offset="122"/>
