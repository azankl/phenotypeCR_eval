                </annotation>
                <annotation id="40">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <location length="11" offset="20"/>
                    <text>iliac wings</text>
                </annotation>
                <annotation id="41">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0100864</infon>
                    <location length="18" offset="44"/>
                    <text>short femoral neck</text>
                </annotation>
                <relation id="39">
                    <infon key="type">webanno.custom.PhenotypeRelations</infon>
