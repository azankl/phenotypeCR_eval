                </annotation>
                <annotation id="7">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0000286</infon>
                    <location length="16" offset="68"/>
                    <text>epicanthic folds</text>
                </annotation>
                <annotation id="8">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0000431</infon>
                    <location length="18" offset="86"/>
