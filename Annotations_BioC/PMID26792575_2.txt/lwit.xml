                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0000232</infon>
                    <location length="17" offset="143"/>
                    <text>everted lower lip</text>
                </annotation>
            </sentence>
            <sentence>
                <offset>679</offset>
                <text>She has small teeth with enamel dysplasia.</text>
                <annotation id="12">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0000691</infon>
                    <location length="11" offset="8"/>
