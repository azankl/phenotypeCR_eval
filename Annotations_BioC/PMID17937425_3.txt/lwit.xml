            <sentence>
                <offset>652</offset>
                <text>Intelligence was normal.</text>
            </sentence>
            <sentence>
                <offset>677</offset>
                <text>Ultrasound studies showed a crossed-fused renal ectopy and true left cryptorchidism.</text>
                <annotation id="16">
                    <infon key="type">webanno.custom.Phenotypes</infon>
                    <infon key="HPOterm">http://purl.obolibrary.org/obo/HP_0004736</infon>
