                    <infon key="Comment">! Typo</infon>
                    <location length="18" offset="43"/>
                    <text>breathing di‰culty</text>
                </annotation>
            </sentence>
            <sentence>
                <offset>980</offset>
                <text>The eyes were&#13;
prominent with protruding eyeballs.</text>
